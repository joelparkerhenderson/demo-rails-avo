<p style="color: green"><%= notice %></p>

<%= search_form_for @q do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name_cont, class: 'form-control' %>
  <%= f.submit %>
<% end %>
  
<h1>Items</h1>

<div id="items">
  <% @items.each do |item| %>
    <%= render item %>
    <p>
      <%= link_to "Show this item", item %>
    </p>
  <% end %>
</div>

<%= link_to "New item", new_item_path %>
